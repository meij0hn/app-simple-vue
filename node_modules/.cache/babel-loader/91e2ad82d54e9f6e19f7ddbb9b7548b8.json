AA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AACA,UAAA,KAAA,CAAA,UAAA,CAAA,KAAA,EAFA,CAEA;;;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,QAAA;;AACA,UAAA,KAAA,CAAA,UAAA;AACA,SARA,EASA,KATA,CASA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AACA,UAAA,KAAA,CAAA,UAAA,CAAA,KAAA,EAFA,CAEA;;;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA,CAAA,OAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAdA;AAeA;AAEA,KAtBA;AAuBA,IAAA,SAvBA,uBAuBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAFA,CAIA;;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,KAAA,IAAA,EAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,CAAA,GAAA,IAAA,EAAA;AACA,OAFA;AAGA,KA/BA;AAgCA,IAAA,UAhCA,wBAgCA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,KAAA;AACA;AAlCA,GAfA;AAmDA,EAAA,QAAA,EAAA;AACA,IAAA,IAAA,EAAA,gBAAA;AACA,aAAA,KAAA,KAAA,GAAA,QAAA,GAAA,QAAA;AACA;AAHA;AAnDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"col-md-4 pl-0\">\r\n    <h2 class=\"mb-5\">Regenerate and Recalculate Balance and Penalty </h2>\r\n    <form @submit.prevent=\"onCreatePost\">\r\n      <div class=\"form-group\">\r\n        <input\r\n          type=\"text\"\r\n          name=\"bookcode\"\r\n          class=\"form-control\"\r\n          placeholder=\"bookcode\"\r\n          v-model=\"form.bookcode\"\r\n          ref=\"fcs\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <div class=\"mt-3 mb-3\">\r\n        <button type=\"submit\" class=\"brn btn-primary\" name=\"action\" value=\"RecalculateBalance\">Recalculate Balance</button>\r\n        <button type=\"submit\" class=\"brn btn-primary\" name=\"action\" value=\"RecalculatePenalty\">Recalculate Penalty</button>\r\n        <button type=\"submit\" class=\"brn btn-primary\" name=\"action\" value=\"RegenerateSchedule\">Regenerate Schedule</button>\r\n      </div>\r\n    \r\n      <div :class=\"flex\">\r\n        <div class=\"alert alert-success\" v-if=\"success === true\">\r\n          <p>Status : {{ info.data.result.status_message }}</p>\r\n        </div>\r\n        <div class=\"alert alert-success\" v-if=\"success === false\">\r\n          {{ info }}\r\n        </div>\r\n      </div>\r\n    \r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nimport Vue from \"vue\";\r\nimport loading from \"vuejs-loading-screen\";\r\n\r\nVue.use(loading, {\r\n  bg: \"#41b883ad\",\r\n  icon: \"refresh\",\r\n  size: 4,\r\n  icon_color: \"white\"\r\n});\r\n\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        bookcode: \"\"\r\n      },\r\n      info: null,\r\n      success: null,\r\n      eror: null,\r\n      mulai: false\r\n    };\r\n  },\r\n  mounted() {\r\n    this.focusInput();\r\n  },\r\n  methods: {\r\n    onCreatePost() {\r\n        this.$isLoading(true); // show loading screen\r\n      \r\n        if ($_POST['action'] == 'RecalculateBalance') {\r\n            axios.post(\"https://api.lippo.homes/api/services/app/PSASSchedule/RecalculateBalanceNew\", {\r\n                \"bookcode\": this.form.bookcode\r\n            })\r\n            .then(response => {\r\n                console.log(response);\r\n                this.$isLoading(false); // hide loading screen\r\n                this.info = response;\r\n                this.focusInput();\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n                this.$isLoading(false); // hide loading screen\r\n                this.info = error.response.data.error.message;\r\n                this.success = false;\r\n            });\r\n        }\r\n\r\n    },\r\n    resetForm() {\r\n      console.log(\"Reset Form\");\r\n      var self = this;\r\n\r\n      //Iterate through each object field, key is name of the object field`\r\n      Object.keys(this.form).forEach(function(key) {\r\n        self.form[key] = \"\";\r\n      });\r\n    },\r\n    focusInput() {\r\n      this.$refs.fcs.focus();\r\n    }\r\n  },\r\n  computed: {\r\n    flex: function() {\r\n      return this.mulai ? \"d-flex\" : \"d-none\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}