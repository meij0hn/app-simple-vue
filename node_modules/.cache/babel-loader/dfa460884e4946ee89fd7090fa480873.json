CAFA;AAGA,QAAA,SAAA,EAAA;AAHA,OAAA,CAAA;AAMA,MAAA,SAAA,CAAA,WAAA,CAAA,MAAA,CAAA,KAAA,IAAA,CAAA,SAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,OAHA;AAIA,KAdA;AAeA,IAAA,SAfA,uBAeA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAFA,CAIA;;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,KAAA,IAAA,EAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,CAAA,GAAA,IAAA,EAAA;AACA,OAFA;AAGA,KAvBA;AAwBA,IAAA,UAxBA,wBAwBA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,KAAA;AACA;AA1BA,GAfA;AA2CA,EAAA,QAAA,EAAA;AACA,IAAA,IAAA,EAAA,gBAAA;AACA,aAAA,KAAA,KAAA,GAAA,QAAA,GAAA,QAAA;AACA;AAHA;AA3CA,CAAA","sourcesContent":["<template>\r\n    <div class=\"col-md-4 pl-0\">\r\n        <h2 class=\"mb-5\">Expire</h2>\r\n    \r\n        <form @submit.prevent=\"onCreatePost\">\r\n        <div class=\"form-group\">\r\n            <input\r\n            type=\"text\"\r\n            name=\"orderCode\"\r\n            class=\"form-control\"\r\n            placeholder=\"orderCode\"\r\n            v-model=\"form.orderCode\"\r\n            ref=\"fcs\"\r\n            required\r\n            />\r\n        </div>\r\n\r\n        <div class=\"mt-3 mb-3\">\r\n            <button type=\"submit\" class=\"brn btn-primary\">Submit</button>\r\n        </div>\r\n\r\n        <div :class=\"flex\">\r\n            <div class=\"alert alert-success\" v-if=\"success === true\">\r\n            <p>Status : {{ info.data.result.status_message }}</p>\r\n            <p>OrderCode : {{ info.data.result.order_id }}</p>\r\n            <p>Amount : {{ info.data.result.gross_amount }}</p>\r\n            <p>Payment Type : {{ info.data.result.payment_type }}</p>\r\n            <p>Transaction Time : {{ info.data.result.transaction_time }}</p>\r\n            <p>Virtual Account : {{ info.data.result.permata_va_number }}</p>\r\n            <p>Transaction Status : {{ info.data.result.transaction_status }}</p>\r\n            </div>\r\n            <div class=\"alert alert-success\" v-if=\"success === false\">\r\n            {{ info }}\r\n            </div>\r\n        </div>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport Vue from \"vue\";\r\nconst midtransClient = require('midtrans-client');\r\n\r\nimport loading from \"vuejs-loading-screen\";\r\nVue.use(loading, {\r\n  bg: \"#41b883ad\",\r\n  icon: \"refresh\",\r\n  size: 4,\r\n  icon_color: \"white\"\r\n});\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        orderCode: \"\"\r\n      },\r\n      info: null,\r\n      success: null,\r\n      eror: null,\r\n      mulai: false\r\n    };\r\n  },\r\n  mounted() {\r\n    this.focusInput();\r\n  },\r\n  methods: {\r\n    onCreatePost() {\r\n      this.$isLoading(true); // show loading screen\r\n      \r\n      let apiClient = new midtransClient.Snap({\r\n          isProduction : false,\r\n          serverKey : 'Mid-server-5SjUm4DPCDKqf_PAMUT2Ht4E',\r\n          clientKey : 'Mid-client-MGKpnVja6krOzV0L'\r\n      });\r\n\r\n      apiClient.transaction.status(this.form.orderCode)\r\n        .then((response)=>{\r\n            console.log(response);\r\n      });\r\n    },  \r\n    resetForm() {\r\n      console.log(\"Reset Form\");\r\n      var self = this;\r\n\r\n      //Iterate through each object field, key is name of the object field`\r\n      Object.keys(this.form).forEach(function(key) {\r\n        self.form[key] = \"\";\r\n      });\r\n    },\r\n    focusInput() {\r\n      this.$refs.fcs.focus();\r\n    }\r\n  },\r\n  computed: {\r\n    flex: function() {\r\n      return this.mulai ? \"d-flex\" : \"d-none\";\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}