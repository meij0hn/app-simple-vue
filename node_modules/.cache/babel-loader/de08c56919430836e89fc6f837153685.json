,CAAA,IAAA,EADA,CACA;;AAEA,MAAA,KAAA,CACA,IADA,CAEA,2DAAA,KAAA,IAAA,CAAA,KAFA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,KAAA,EAFA,CAEA;;;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,QAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,IAAA;;AACA,QAAA,KAAA,CAAA,SAAA,GANA,CAMA;;;AACA,QAAA,KAAA,CAAA,UAAA;AACA,OAZA,EAaA,KAbA,CAaA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,KAAA,EAFA,CAEA;;;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,KAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAnBA;AAqBA,KAzBA;AA0BA,IAAA,SA1BA,uBA0BA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAFA,CAIA;;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,KAAA,IAAA,EAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,CAAA,GAAA,IAAA,EAAA;AACA,OAFA;AAGA,KAlCA;AAmCA,IAAA,UAnCA,wBAmCA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,KAAA;AACA;AArCA,GAfA;AAsDA,EAAA,QAAA,EAAA;AACA,IAAA,IAAA,EAAA,gBAAA;AACA,aAAA,KAAA,KAAA,GAAA,QAAA,GAAA,QAAA;AACA;AAHA;AAtDA,CAAA","sourcesContent":["<template>\r\n    <div class=\"col-md-4 pl-0\">\r\n        <h2 class=\"mb-5\">Expire</h2>\r\n    \r\n        <form @submit.prevent=\"onCreatePost\">\r\n        <div class=\"form-group\">\r\n            <input\r\n            type=\"text\"\r\n            name=\"order\"\r\n            class=\"form-control\"\r\n            placeholder=\"order\"\r\n            v-model=\"form.order\"\r\n            ref=\"fcs\"\r\n            required\r\n            />\r\n        </div>\r\n\r\n        <div class=\"mt-3 mb-3\">\r\n            <button type=\"submit\" class=\"brn btn-primary\">Submit</button>\r\n        </div>\r\n\r\n        <div :class=\"flex\">\r\n            <div class=\"alert alert-success\" v-if=\"success === true\">\r\n            <p>Status : {{ info.data.status_message }}</p>\r\n            <p>OrderCode : {{ info.data.order_id }}</p>\r\n            <p>Amount : {{ info.data.gross_amount }}</p>\r\n            <p>Payment Type : {{ info.data.payment_type }}</p>\r\n            <p>Transaction Time : {{ info.data.transaction_time }}</p>\r\n            <p>Virtual Account : {{ info.data.permata_va_number }}</p>\r\n            <p>Transaction Status : {{ info.data.transaction_status }}</p>\r\n            </div>\r\n            <div class=\"alert alert-success\" v-if=\"success === false\">\r\n            {{ info }}\r\n            </div>\r\n        </div>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport Vue from \"vue\";\r\n\r\n\r\nimport loading from \"vuejs-loading-screen\";\r\nVue.use(loading, {\r\n  bg: \"#41b883ad\",\r\n  icon: \"refresh\",\r\n  size: 4,\r\n  icon_color: \"white\"\r\n});\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        order: \"\"\r\n      },\r\n      info: null,\r\n      success: null,\r\n      eror: null,\r\n      mulai: false\r\n    };\r\n  },\r\n  mounted() {\r\n    this.focusInput();\r\n  },\r\n  methods: {\r\n    onCreatePost() {\r\n      this.$isLoading(true); // show loading screen\r\n      \r\n      axios\r\n        .post(\r\n          \"http://mrmjohn.com:3002/api/midtrans/status?ordercode=\" + this.form.order\r\n        )\r\n        .then(response => {\r\n          console.log(response);\r\n          this.$isLoading(false); // hide loading screen\r\n          this.mulai = true;\r\n          this.info = response;\r\n          this.success = true;\r\n          this.resetForm(); // reset\r\n          this.focusInput();\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n          this.$isLoading(false); // hide loading screen\r\n          this.mulai = true;\r\n          this.info = false;\r\n          this.success = false;\r\n        });\r\n\r\n    },  \r\n    resetForm() {\r\n      console.log(\"Reset Form\");\r\n      var self = this;\r\n\r\n      //Iterate through each object field, key is name of the object field`\r\n      Object.keys(this.form).forEach(function(key) {\r\n        self.form[key] = \"\";\r\n      });\r\n    },\r\n    focusInput() {\r\n      this.$refs.fcs.focus();\r\n    }\r\n  },\r\n  computed: {\r\n    flex: function() {\r\n      return this.mulai ? \"d-flex\" : \"d-none\";\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}